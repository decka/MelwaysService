<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>MelwaysService</title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
    <script src="http://code.jquery.com/jquery-latest.js"></script>
    <script src='@Url.Content("~/Scripts/knockout-3.1.0.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Scripts/json2.min.js")' type="text/javascript"></script>
</head>
<body>
    <form data-bind="submit: addressSearch">
        <input data-bind="value: addressText" placeholder="What are you looking for?" />
        <button type="submit">Search</button>
    </form>
    <br />
    <table class="table table-hover">
        <tbody data-bind="foreach: melwaysResult">
            <tr>
                <td><span data-bind="text: Address"></span></td>
                <td><span data-bind="text: MelwaysReference"></span></td>
            </tr>
        </tbody>
    </table>
    <script src='@Url.Content("~/Scripts/MelwaysResultViewModel.js")' type="text/javascript"></script>
</body>
</html>
